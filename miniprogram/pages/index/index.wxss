/* pages/index/index.wxss */

.box {
  display: flex;
  justify-content: center;
  background-color: #ffd51b;
  margin: 20rpx;
  padding: 20rpx;
  border: 1rpx solid #202020;
  border-radius: 20rpx;
}

.list_box {
  display: flex;
  flex-direction: column;
}

.list_contest {
  width: 100%;
  display: flex;
  flex-direction: row;
  margin: 10rpx;
}

.list_contest_content {
  display: flex;
  flex-direction: column;
  align-self: flex-start;
  margin: 10rpx;
}

.list_contest_delete {
  margin-left: auto;
  align-self: center;
}

.list_submit {
  display: flex;
  margin: 10rpx;
}

.list_submit text {
  margin: 10rpx;
}

.list_submit input {
  margin: 10rpx;
}

